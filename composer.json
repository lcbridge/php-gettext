{
    "name": "lcbridge/php-gettext",
    "description": "Composer configuration to fetch php-gettext non-packagist repository by Danilo Segan",
    "keywords": ["php", "gettext"],
    "authors": [
        {
            "name": "Lcbrige",
            "email": "glenwiriawan77@gmail.com"
        }
    ],
    "repositories": [
        {
            "type": "package",
            "package": {  
                "name": "php-gettext/php-gettext",
                "version": "1.0.11",
                "dist": {
                    "type": "tar",
                    "url": "https://launchpad.net/php-gettext/trunk/1.0.11/+download/php-gettext-1.0.11.tar.gz"
                },
                "autoload": {
                    "classmap": [
                        "gettext.php",
                        "streams.php"
                    ],
                    "files": [
                        "gettext.inc"
                    ]
                }
            }
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "php-gettext/php-gettext": "1.0.*"
    }
}
